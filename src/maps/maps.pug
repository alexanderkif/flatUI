mixin map(lat, lng)
    div.map
        //- iframe(src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d12611.823497676294!2d"+lng+"!3d"+lat+"!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x80858092e7ee4833%3A0xba9adf323eff9155!2zMTI1OSBDYWxpZm9ybmlhIFN0LCBTYW4gRnJhbmNpc2NvLCBDQSA5NDEwOSwg0KHQqNCQ!5e0!3m2!1sru!2sru!4v1533790748152" width="590" height="200" frameborder="0" style="border:0" allowfullscreen)
        iframe(src="https://maps.google.com/maps?q="+lat+", "+lng+"&z=15&output=embed" width="590" height="200" frameborder="0" style="border:0")
        .map-bottom
            .map-slog= "Meet us!"
            .map-address
                .map-address= "1259 california st"
                .map-address1= "san francisco, ca"
            .map-point
                i.material-icons= "location_searching"
            .map-target
                i.material-icons= "location_on"
                